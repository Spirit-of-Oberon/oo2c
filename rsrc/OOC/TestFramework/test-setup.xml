<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE test-setup SYSTEM "test-setup.dtd">

<test-setup lang="O1">
  <machine model="IBM ThinkPad 600E"
	   cpu="Mobile Pentium II, stepping 10"
	   ram="288 MB" />
  
  <operating-system name="Linux"
		    version="2.4.2-pre4 i686 unknown">
    <os-component name="libc">/lib/libc-2.2.2.so</os-component>
    <os-component name="libgc">unknown</os-component>
    <os-component name="cc">gcc 2.95.2</os-component>
  </operating-system>
    
  <processor name="oo2c"
	     version="1.5.5.1"
	     command="/usr/bin/Oo2c"
	     config-file="./sample-oo2crc"
	     flags="-Ov"
	     working-dir="/tmp/ooc-tests" 
	     filter-command="/usr/bin/Ooef" />

  <test-suite uri="testsuite.xml" />
</test-setup>
