<test-parse-uri><uri-string>http://user@host.com:8080/furbie;test?foo=bar</uri-string>

<hierarchical-uri>
<scheme>http</scheme>
<authority-server-based>
<userinfo>user</userinfo>
<host>host.com</host>
<port>8080</port>
</authority-server-based>
<absolute-path>
<segment>
<value>furbie</value>
<parameter>test</parameter>
</segment>
</absolute-path>
<query-unparsed>foo=bar</query-unparsed>
</hierarchical-uri>
</test-parse-uri>
