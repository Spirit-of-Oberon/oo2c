<?xml version="1.0" encoding="ISO-8859-1"?>

<testcases profile="OOC Core Library" default-lang="O2">
  <test id="libadt_Object" type="run" file="LibAdtObject.Mod">
    Imports the module <code>ADT:Object</code>.
  </test>
  <test id="libadt_String" type="run"
        file="LibAdtString.Mod" output="LibAdtString.Ref">
    Imports the module <code>ADT:String</code>, and performs some
    operations on string instances.
  </test>
  <test id="libadt_StringBuffer" type="run"
        file="LibAdtStringBuffer.Mod" output="LibAdtStringBuffer.Ref">
    Imports the module <code>ADT:StringBuffer</code>, and performs
    some operations on buffer instances.
  </test>
  <test id="libadt_Dictionary" type="run" file="LibAdtDictionary.Mod">
    Imports the various variants of module <code>ADT:Dictionary</code>.
  </test>
  <test id="libadt_StringSearch1" type="run" file="LibStringSearch1.Mod">
    Imports module <code>StringSearch</code> and derived modules.
  </test>
</testcases>
